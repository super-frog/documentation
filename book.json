{
  "title": "super frog",
  "author": "lanhao",
  "description": "+1s",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "structure": {
      "readme": "README.md"
  },
  "links": {
      "sidebar": {
          "开发者小蓝": "http://www.lanhao.name"
      }
  },
  "plugins": [
      
      "-sharing",
      "splitter",
      "expandable-chapters-small",
      "anchors",
      "github-buttons",
      "-highlight",
      "prism",
      "prism-themes",
      "-github"
  ],
  "pluginsConfig": {
      "github": {
          "url": "https://github.com/super-frog/frog-cmd"
      },
      "prism": {
        "css": [
          "prism-themes/themes/prism-xonokai.css"
        ]
      },
      "github-buttons": {
          "buttons": [{
              "user": "super-frog",
              "repo": "frog-cmd",
              "type": "star",
              "size": "small",
              "count": true
              }
          ]
      }
  }
